<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor>
    <parser>
        <title>Bot Blocker</title>
        <target>docs</target>
        <markers>
            <item>TODO</item>
            <item>FIXME</item>
        </markers>
    </parser>
    <transformer>
        <target>/tmp/phpdoc-demo/responsive</target>
    </transformer>
    <files>
        <directory>.</directory>
        <ignore>tests/*</ignore>
        <ignore>vendor/*</ignore>
    </files>
    <plugins>
        <plugin path="Core" />
    </plugins>
    <transformations>
        <template name="clean" />
    </transformations>
</phpdocumentor>
